{
  "manifest_version": 2,
  "name": "inject dom demo step1",
  "version": "0.0.1",
  "description": "注入标签到页面",
  "permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "images/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "/content_scripts/content_script.js"
      ],
      "all_frames": true,
      "run_at": "document_idle"
    }
  ]
}